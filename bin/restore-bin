#!/bin/bash

CROUTON_DIR=/var/crouton
sudo cp -a /media/removable/Backup/bin/.                    "${CROUTON_DIR}/bin"
sudo cp -a /media/removable/Backup/crouton/bin/.            "${CROUTON_DIR}/bin"
sudo cp -a /media/removable/Backup/chromeos/usr/local/bin/. "${CROUTON_DIR}/bin"
sudo cp -a /media/removable/Backup/bin/.                    /usr/local/bin
sudo cp -a /media/removable/Backup/crouton/bin/.            /usr/local/bin
sudo cp -a /media/removable/Backup/chromeos/usr/local/bin/. /usr/local/bin
